{
  "name": "pdf-comment-app",
  "version": "2.0.0",
  "description": "AplicaciÃ³n web para recibir comentarios visuales sobre documentos PDF",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "create-project": "node scripts/create-project.js",
    "init-db": "node scripts/init-db.js",
    "init-db-sample": "node scripts/init-db.js --sample-data",
    "healthcheck": "node scripts/healthcheck.js",
    "healthcheck-docker": "node scripts/healthcheck.js --docker",
    "healthcheck-json": "node scripts/healthcheck.js --json",
    "list-projects": "node scripts/create-project.js list",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t pdf-comments-app .",
    "docker:run": "docker run -p 3001:3001 pdf-comments-app",
    "docker:compose": "docker-compose up -d",
    "docker:compose-build": "docker-compose up --build -d",
    "docker:compose-down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "backup": "tar -czf backup-$(date +%Y%m%d-%H%M%S).tar.gz database.db uploads/",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "pdf",
    "comments",
    "feedback",
    "review",
    "collaboration",
    "web-app",
    "nodejs",
    "react",
    "sqlite"
  ],
  "author": "PDF Comments App Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/pdf-comments-app.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/pdf-comments-app/issues"
  },
  "homepage": "https://github.com/tu-usuario/pdf-comments-app#readme",
  "config": {
    "unsafe-perm": true
  }
}